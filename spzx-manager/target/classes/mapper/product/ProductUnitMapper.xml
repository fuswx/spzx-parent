<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fuswx.spzx.manager.mapper.ProductUnitMapper">

    <!-- 查找所有 -->
    <select id="findAll" resultType="com.fuswx.spzx.model.entity.base.ProductUnit">
        select *
        from product_unit
        where is_deleted = 0
        order by id
    </select>

</mapper>